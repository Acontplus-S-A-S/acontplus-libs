{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "acontplus-customers",
  "projectType": "library",
  "sourceRoot": "projects/acontplus-customers/src",
  "prefix": "lib",
  "targets": {
    "build": {
      "executor": "@angular/build:ng-packagr",
      "configurations": {
        "production": {
          "tsConfig": "projects/acontplus-customers/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "projects/acontplus-customers/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@angular/build:karma",
      "options": {
        "tsConfig": "projects/acontplus-customers/tsconfig.spec.json",
        "polyfills": [
          "zone.js",
          "zone.js/testing"
        ]
      }
    }
  }
}