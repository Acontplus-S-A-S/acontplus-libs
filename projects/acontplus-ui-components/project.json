{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "acontplus-ui-components",
  "projectType": "library",
  "sourceRoot": "projects/acontplus-ui-components/src",
  "prefix": "acp",
  "generators": {
    "@schematics/angular:component": {
      "style": "scss",
      "prefix": "acp"
    },
    "@schematics/angular:directive": {
      "prefix": "acp"
    }
  },
  "targets": {
    "build": {
      "executor": "@angular/build:ng-packagr",
      "options": {
        "project": "projects/acontplus-ui-components/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "projects/acontplus-ui-components/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "projects/acontplus-ui-components/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production",
      "outputs": [
        "{workspaceRoot}/dist/acontplus-ui-components"
      ]
    },
    "test": {
      "executor": "@angular-devkit/build-angular:jest",
      "options": {
        "tsConfig": "projects/acontplus-ui-components/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@angular-eslint/builder:lint",
      "options": {
        "lintFilePatterns": [
          "projects/acontplus-ui-components/**/*.ts",
          "projects/acontplus-ui-components/**/*.html"
        ],
        "eslintConfig": "eslint.config.js"
      },
      "outputs": [
        "{options.outputFile}"
      ]
    }
  }
}