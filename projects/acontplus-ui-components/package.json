{
  "name": "acontplus-ui-components",
  "version": "1.2.4",
  "description": "Angular Material UI component library with dynamic tables, theming support, dialog wrappers, customer management components, and comprehensive styling utilities",
  "scripts": {
    "format": "prettier --write \"**/*.{ts,js,html,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,html,css,scss,json,md}\"",
    "format:fix": "npm run format"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "@angular/common": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/material": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "acontplus-core": "^1.2.6"
  },
  "exports": {
    "./styles": "./styles/_index.scss",
    "./styles/buttons": "./styles/_custom-buttons.scss",
    "./styles/dialog": "./styles/_custom-dialog.scss",
    "./styles/snackbar": "./styles/_snackbar-theme.scss"
  },
  "sideEffects": [
    "**/*.scss",
    "**/*.css"
  ],
  "main": "fesm2022/acontplus-ui-components.mjs",
  "module": "fesm2022/acontplus-ui-components.mjs",
  "typings": "index.d.ts",
  "keywords": [
    "acontplus",
    "angular",
    "angular-material",
    "material-design",
    "ui-components",
    "dynamic-table",
    "tabulator",
    "cards",
    "dialog",
    "snackbar",
    "theme",
    "theming",
    "dark-mode",
    "overlay",
    "autocomplete",
    "spinner",
    "interceptors",
    "customer-ui",
    "form-controls",
    "directives",
    "pipes",
    "scss",
    "styling",
    "components",
    "typescript",
    "frontend",
    "library"
  ],
  "author": "Ivan Paz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Acontplus-S-A-S/acontplus-libs/issues"
  },
  "homepage": "https://github.com/Acontplus-S-A-S/acontplus-libs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Acontplus-S-A-S/acontplus-libs.git"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
