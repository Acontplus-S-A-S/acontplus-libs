{
  "name": "acontplus-core",
  "version": "1.1.0",
  "description": "Core utilities, services, and models for Angular applications",
  "scripts": {
    "format": "prettier --write \"**/*.{ts,js,html,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,html,css,scss,json,md}\"",
    "format:fix": "npm run format"
  },
  "dependencies": {
    "jwt-decode": "^4.0.0",
    "ngx-toastr": "^19.0.0",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "@angular/common": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/router": "^20.0.0",
    "rxjs": "^7.8.0"
  },
  "peerDependenciesMeta": {
    "@angular/router": {
      "optional": true
    }
  },
  "sideEffects": false,
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "files": [
    "index.js",
    "index.d.ts",
    "*.d.ts",
    "**/*.d.ts"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "acontplus",
    "core",
    "utils",
    "services",
    "interceptors",
    "models",
    "pipes",
    "environments",
    "angular",
    "typescript",
    "frontend",
    "common",
    "shared",
    "library",
    "helpers"
  ],
  "author": "Ivan Paz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Acontplus-S-A-S/acontplus-libs/issues"
  },
  "homepage": "https://github.com/Acontplus-S-A-S/acontplus-libs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Acontplus-S-A-S/acontplus-libs.git"
  }
}
