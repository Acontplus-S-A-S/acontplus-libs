{
  "name": "acontplus-libs",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps",
    "packages/*"
  ],
  "scripts": {
    "start": "nx serve demo-app --ssl true --no-hmr",
    "build": "nx build demo-app",
    "watch": "nx build demo-app --watch --configuration development",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "build:all": "nx run-many --target=build --all",
    "build:libs": "nx run-many --target=build --projects=core,ng-components,ng-core,ng-customer,ng-notifications,ui-kit,utils",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "prettier --write \"**/*.{ts,js,html,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,html,css,scss,json,md}\"",
    "format:fix": "npm run format",
    "lint:all": "npm run lint && npm run format:check",
    "clean": "nx reset",
    "local-registry": "nx run acontplus-libs:local-registry",
    "e2e": "nx run demo-app-e2e:e2e"
  },
  "dependencies": {
    "@acontplus/core": "^1.0.3",
    "@acontplus/ng-components": "^1.0.4",
    "@acontplus/ng-core": "^1.0.3",
    "@acontplus/ng-notifications": "^1.0.0",
    "@acontplus/utils": "^1.0.1",
    "@angular/animations": "^20.3.1",
    "@angular/cdk": "^20.2.4",
    "@angular/common": "^20.3.1",
    "@angular/compiler": "^20.3.1",
    "@angular/core": "^20.3.1",
    "@angular/forms": "^20.3.1",
    "@angular/material": "^20.2.4",
    "@angular/platform-browser": "^20.3.1",
    "@angular/platform-browser-dynamic": "^20.3.1",
    "@angular/router": "^20.3.1",
    "@jsverse/transloco": "^8.0.0",
    "@jsverse/utils": "^1.0.0-beta.6",
    "bootstrap": "^5.3.8",
    "decimal.js": "^10.6.0",
    "jwt-decode": "^4.0.0",
    "ngx-toastr": "^19.1.0",
    "rxjs": "^7.8.2",
    "sweetalert2": "^11.23.0",
    "tabulator-tables": "^6.3.1",
    "tslib": "^2.8.1",
    "uuid": "^13.0.0",
    "zone.js": "^0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.2",
    "@angular-devkit/core": "^20.3.2",
