{
  "name": "acontplus-libs",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps",
    "packages/*"
  ],
  "scripts": {
    "start": "nx serve demo-app --ssl true --hmr",
    "build": "nx build demo-app",
    "watch": "nx build demo-app --watch --configuration development",
    "test": "nx run-many --target=test --all",
    "test:watch": "nx run-many --target=test --all --watch",
    "test:coverage": "nx run-many --target=test --all --coverage",
    "build:all": "nx run-many --target=build --all",
    "build:libs": "nx run-many --target=build --projects=core,ng-components,ng-core,ng-customer,ng-notifications,ui-kit,utils",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "prettier --write \"**/*.{ts,js,html,css,scss,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,html,css,scss,json,md}\"",
    "format:fix": "npm run format",
    "lint:all": "npm run lint && npm run format:check",
    "clean": "nx reset",
    "local-registry": "nx run acontplus-libs:local-registry",
    "e2e": "nx run demo-app-e2e:e2e"
  },
  "dependencies": {
