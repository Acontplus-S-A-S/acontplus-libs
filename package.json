    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.1",
    "@jest/globals": "^30.1.2",
    "@nx/angular": "21.5.3",
    "@nx/devkit": "21.5.3",
    "@nx/jest": "21.5.3",
    "@nx/js": "^21.5.3",
    "@nx/playwright": "21.5.3",
    "@nx/vite": "21.5.3",
    "@nx/web": "21.5.3",
    "@nx/workspace": "21.5.3",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "^20.3.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^30.0.0",
